<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="formLogin" alignment="CENTER" prefHeight="240.0" prefWidth="422.0" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.LoginController">
   <children>
      <Label text="Login View" />
      <TextField fx:id="email_input" promptText="Email">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <TextField promptText="Senha" />
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button mnemonicParsing="false" prefHeight="27.0" prefWidth="405.0" text="Ir para pagina de cadastro" onAction="#switchToRegisterPage" />
         </children>
      </HBox>
      <Button fx:id="primaryButton" prefHeight="27.0" prefWidth="412.0" text="Fazer login" textAlignment="CENTER" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</VBox>
